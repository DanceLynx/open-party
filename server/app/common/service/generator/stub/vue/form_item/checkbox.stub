<el-form-item label="{COLUMN_COMMENT}" prop="{COLUMN_NAME}">
    <el-checkbox-group v-model="formData.{COLUMN_NAME}" placeholder="请选择{COLUMN_COMMENT}">
        <el-checkbox 
            v-for="(item, index) in dictData.{DICT_TYPE}"
            :key="index" 
            :label="item.value"
        >
            {{ item.name }}
        </el-checkbox>
    </el-checkbox-group>
</el-form-item>