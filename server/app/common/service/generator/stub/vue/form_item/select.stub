<el-form-item label="{COLUMN_COMMENT}" prop="{COLUMN_NAME}">
    <el-select class="flex-1" v-model="formData.{COLUMN_NAME}" clearable placeholder="请选择{COLUMN_COMMENT}">
        <el-option 
            v-for="(item, index) in dictData.{DICT_TYPE}"
            :key="index" 
            :label="item.name"
            :value="{ITEM_VALUE}"
        />
    </el-select>
</el-form-item>